{
	"servers": {
		"awesome-copilot": {
			"type": "stdio",
			"command": "docker",
			"args": [
				"run",
				"-i",
				"--rm",
				"ghcr.io/microsoft/mcp-dotnet-samples/awesome-copilot:latest"
			]
		},
		"terraform": {
			"command": "docker",
			"args": [
				"run",
				"-i",
				"--rm",
				"hashicorp/terraform-mcp-server"
			],
			"type": "stdio"
		},
		"github": {
			"url": "https://api.githubcopilot.com/mcp/",
			"type": "http"
		}
	},
	"inputs": []
}